{
 "version": "0.0.0",
 "name": "hindsite",
 "manifest_version": 2,
 "description": "hindsite chrome extension",
 "browser_action": {
   "default_title": "hindsite",
   "default_popup": "/popup.html"
 },
 "background": {
  "scripts": ["scripts/background.js"]
},
 "icons": {
   "16": "assets/img/icon-16.png",
   "48": "assets/img/icon-48.png",
   "128": "assets/img/icon-128.png"
 },
 "permissions": [
   "tabs",
   "sessions",
   "http://127.0.0.1:8000/*"
 ]
}
